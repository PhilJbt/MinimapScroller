{
  "manifest_version": 3,
  "name": "DOM Minimap",
  "version": "1.0",
  "description": "Displays a scrollable minimap of the DOM on the right side of the page.",
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}
